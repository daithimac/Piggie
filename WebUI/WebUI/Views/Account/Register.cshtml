@model WebUI.Models.RegisterModel
<link rel="stylesheet" type="text/css" href="~/Content/Style1.css" />
<link rel="stylesheet" type="text/css" href="~/Content/animate-custom.css" />   
    

<!--    <fieldset><a href="~/Views/Account/_ExternalLoginsListPartial.cshtml">~/Views/Account/_ExternalLoginsListPartial.cshtml</a>
        <legend>Log in Form</legend>-->
        <div id="container_demo" >
    <a class="hiddenanchor" id="toregister"></a>
    <a class="hiddenanchor" id="tologin"></a>
   <div id="wrapper">

     <div id="login" class="animate form">
            <form  action="mysuperscript.php" autocomplete="on">
                <h1> Sign up </h1>
                <p>
                @Html.LabelFor(m => m.Email)
                @Html.TextBoxFor(m => m.Email)
                </p>
                <p>
                @Html.LabelFor(m => m.Forename)
                @Html.TextBoxFor(m => m.Forename)
                </p>
                <p>
                @Html.LabelFor(m => m.Surname)
                @Html.TextBoxFor(m => m.Surname)
                </p>
                @Html.LabelFor(m => m.Password)
                @Html.PasswordFor(m => m.Password)
                </p>
                <p>
                @Html.LabelFor(m => m.ConfirmPassword)
                @Html.PasswordFor(m => m.ConfirmPassword)
                </p>
                <p class="login button">
                    <input type="submit" value="Register" />
                <p class="change_link"> 
                    Already a member? @Html.ActionLink("Login", "Login") 
                </p>
            </form>
        </div>
         
    </div>
</div>  
}
